{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "env": {
    "LLM_MODE": "api"
  },
  "build": {
    "env": {
      "LLM_MODE": "api"
    }
  },
  "rewrites": [
    {
      "source": "/agents/:agent/.well-known/agent-card.json",
      "destination": "/api/agents/card?agent=:agent"
    },
    {
      "source": "/agents/:agent/rpc",
      "destination": "/api/agents/rpc?agent=:agent"
    },
    {
      "source": "/agents/:agent/health",
      "destination": "/api/agents/health?agent=:agent"
    },
    {
      "source": "/tools",
      "destination": "/api/tools"
    },
    {
      "source": "/tools/health",
      "destination": "/api/tools/health"
    },
    {
      "source": "/tools/call",
      "destination": "/api/tools/call"
    },
    {
      "source": "/",
      "destination": "/api/index"
    }
  ]
}
